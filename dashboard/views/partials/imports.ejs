<!-- Cookies -->
<link rel="stylesheet" href="/static/css/cookies.css">
<div id="cookie-info" class="cookie-info">
    <div class="content">
        <h1>Cookies</h1>
        <br>
        <p>Informujemy iż wymagamy dostępu do plików cookies. Wiecej w polityce prywatności</p>
        <a class="privacy" href="/privacy">Polityka Prywatności</a>
        <br>
        <br>
        <a href="javascript:cookie_notify_accept('cookie_notify',1,7);"
           class="button">Akceptuję</a>
    </div>
</div>
<script>
    const cookie_notify_accept = (name, value, days) => {
        let expires;
        if (days) {
            const date = new Date();
            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
            expires = "; expires=" + date.toGMTString();
        } else expires = "";
        document.cookie = name + "=" + value + expires + "; path=/";
        document.getElementById("cookie-info").style.display = "none";
    }

    const cookie_notify_check = (name) => {
        const nameEQ = name + "=";
        const ca = document.cookie.split(";");
        for (let i = 0; i < ca.length; i++) {
            let c = ca[i];
            while (c.charAt(0) === " ") c = c.substring(1, c.length);
            if (!c.indexOf(nameEQ)) return c.substring(nameEQ.length, c.length);
        }
    }

    if (cookie_notify_check("cookie_notify")) document.getElementById("cookie-info").style.display = "none";
</script>
